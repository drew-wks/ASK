{
    "githubPullRequests.ignoredPullRequestBranches": [
        "main"
    ],
    "workbench.colorCustomizations": {
        "activityBar.background#": "#FFE873",
        "activityBar.foreground#": "#306998",
        "#activityBar.background": "#323330",
        "#activityBar.foreground": "#F0DB4F",
        "statusBar.background": "#323330",
        "statusBar.foreground": "#F0DB4F"
    },
    "python.testing.pytestEnabled": true,
    "python.testing.pytestArgs": [
        "-m", "pytest"
    ],
    "python.testing.unittestEnabled": false,
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv-main/bin/python",
}